import {
  CreateDateColumn,
  DeleteDateColumn,
  Entity,
  PrimaryGeneratedColumn,
  UpdateDateColumn,
} from 'typeorm';

@Entity()
export default class {{properCase name}} {
  @PrimaryGeneratedColumn('uuid')
  id!: string;

  @CreateDateColumn({
    precision: 3,
    type: 'timestamptz',
  })
  createdAt!: Date;

  @UpdateDateColumn({
    precision: 3,
    type: 'timestamptz',
  })
  updatedAt!: Date;

  @DeleteDateColumn({
    precision: 3,
    type: 'timestamptz',
  })
  deletedAt?: Date;
}
